{
    "name": "purebrainz",
    "displayName": "PureBrainz",
    "description": "Syntax highlighting for the pb language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "purebrainz",
            "aliases": ["PureBrainz", "purebrainz"],
            "extensions": [".pb"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "purebrainz",
            "scopeName": "",
            "path": "./syntaxes/purebrainz.tmLanguage.json"
        }]
    }
}